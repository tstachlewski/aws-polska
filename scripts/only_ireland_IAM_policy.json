{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DenyOutsideIreland",
            "Effect": "Deny",
            "Action": "*",
            "Resource": "*",
            "Condition": {
                "StringNotEquals": {
                    "aws:RequestedRegion": [
                        "eu-west-1"
                    ]
                }
            }
        },
        {
            "Sid": "DenyAccessToIAM",
            "Effect": "Deny",
            "Action": "iam:*",
            "Resource": "*"
        }
    ]
}
